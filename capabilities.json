{
	"dataRoles": [
		{
			"displayName": "Ids",
			"name": "ids",
			"kind": "GroupingOrMeasure"
		},
		{
			"displayName": "File Id",
			"name": "file_id",
			"kind": "Measure",
			"preferredTypes": [
				{
					"text": true
				}
			]
		},
		{
			"displayName": "API Key",
			"name": "api_key",
			"kind": "Measure",
			"preferredTypes": [
				{
					"text": true
				}
			]
		},
		{
			"displayName": "Server URL",
			"name": "server_url",
			"kind": "Measure",
			"preferredTypes": [
				{
					"text": true
				}
			]
		}
	],
	"objects": {
		"dataPoint": {
			"properties": {
				"defaultColor": {
					"type": {
						"fill": {
							"solid": {
								"color": true
							}
						}
					}
				},
				"showAllDataPoints": {
					"type": {
						"bool": true
					}
				},
				"fill": {
					"type": {
						"fill": {
							"solid": {
								"color": true
							}
						}
					}
				},
				"fillRule": {
					"type": {
						"fill": {}
					}
				},
				"fontSize": {
					"type": {
						"formatting": {
							"fontSize": true
						}
					}
				}
			}
		}
	},
	"dataViewMappings": [
		{
			"conditions": [
				{
					"ids": { "min": 1 },
					"file_id": { "max": 1 },
					"api_key": { "max": 1 },
					"server_url": { "max": 1 }
				}
			],
			"table": {
				"rows": {
					"dataReductionAlgorithm": {
						"top": {
							"count": 30000
						}
					},
					"select": [
						{ "for": { "in": "ids" } },
						{ "bind": { "to": "file_id" } },
						{ "bind": { "to": "api_key" } },
						{ "bind": { "to": "server_url" } }
					]
				}
			}
		}
	],
	"supportsMultiVisualSelection": true,
	"supportsHighlight": true,
	"privileges": [
		{
			"name": "WebAccess",
			"essential": true,
			"parameters": ["http://localhost:3000", "https://pbi-server.fastbim5.eu"]
		}
	]
}
